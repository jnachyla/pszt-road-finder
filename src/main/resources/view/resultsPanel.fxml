<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane id="resultsModalGridPane" fx:controller="com.github.roadfinder.controler.ResultControllerImpl"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Label fx:id="label1" text="Maksymalna generacja" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="maxGenerationTextField" text="100" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    <Label fx:id="label2" text="Rozmiar populacji" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <TextField fx:id="populationSizeTextField" text="20" GridPane.columnIndex="0" GridPane.rowIndex="3"/>

    <Label fx:id="label22" text="Waga funkcji celu dla mas" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <TextField fx:id="sadas" text="1" GridPane.columnIndex="0" GridPane.rowIndex="5"/>

    <Label fx:id="label23" text="Waga funkcji celu dla ścieżek" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
    <TextField fx:id="asda" text="1" GridPane.columnIndex="0" GridPane.rowIndex="7"/>

    <Button text="Znajdź ściężkę" onAction="#findRoad" GridPane.columnIndex="0" GridPane.rowIndex="8"/>

</GridPane>